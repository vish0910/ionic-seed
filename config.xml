<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="io.egen.svs" version="0.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>MoneyUp</name>
  <description>
        &gt; Our app will help you set and monitor budget for various categories you spend money on (viz. Food,
        Clothing, Shelter etc.).
        &gt; Our app will also help you maintain or build your credit history by indicating which credit-card you should
        use to maintain a healthy ration between the amount spent and the credit limit of that specific credit card.
        &gt; It will help you maintain your good credit standing by reminding you to pay your Bills ( Credit card,
        Utilities, Insurance and Rent) in timely manner.
        &gt; For all this analysis and convenience, all you have to do is record the daily transactions.
    </description>
  <author email="vdoshi@egen.solutions" href="https://egen.solutions">
        Vishal Doshi
    </author>
  <content src="index.html"/>
  <access origin="*"/>
  <allow-navigation href="*"/>
  <allow-intent href="*"/>
  <preference name="webviewbounce" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="android-minSdkVersion" value="16"/>
  <preference name="android-targetSdkVersion" value="22"/>
  <preference name="BackupWebStorage" value="none"/>
  <preference name="SplashScreen" value="screen"/>
  <preference name="orientation" value="portrait"/>
  <preference name="AutoHideSplashScreen" value="true"/>
  <preference name="ShowSplashScreenSpinner" value="false"/>
  <preference name="FadeSplashScreen" value="true"/>
  <preference name="FadeSplashScreenDuration" value="1000"/>
  <preference name="KeyboardDisplayRequiresUserAction" value="false"/>
  <preference name="SplashScreenDelay" value="3000"/>
  <preference name="NSPhotoLibraryUsageDescription" value="Test message"/>
  <feature name="StatusBar">
